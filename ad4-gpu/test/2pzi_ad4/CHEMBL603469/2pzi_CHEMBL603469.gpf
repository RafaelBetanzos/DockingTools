npts 25 25 25                        # num.grid points in xyz
gridfld 2pzi.maps.fld                # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD        # receptor atom types
ligand_types C HD OA N               # ligand atom types
receptor 2pzi.pdbqt                  # macromolecule
gridcenter -0.267 1.219 -0.100       # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map 2pzi.C.map                       # atom-specific affinity map
map 2pzi.HD.map                      # atom-specific affinity map
map 2pzi.OA.map                      # atom-specific affinity map
map 2pzi.N.map                       # atom-specific affinity map
elecmap 2pzi.e.map                   # electrostatic potential map
dsolvmap 2pzi.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
